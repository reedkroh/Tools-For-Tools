<!--LIST OF TOOLS FOUND UNDER THE CATEGORY-->
<!--EACH TOOL WILL LINK TO tool.handlebars WHICH IS A CARD OF IT'S INFORMATION-->

<div class="container">
  <div class="row">
    <h1>Categories of Tools</h1>
          <h3>Examples:</h3>
          <ul id="example-list" class="list-group">
            {{#each examples}}
            <li data-id="{{this.id}}" class="list-group-item">
              <a href="example/{{this.id}}">{{this.tool}}</a>
              <button class="btn btn-danger float-right delete">ï½˜</button>
            </li>
            {{/each}}
          </ul>
  </div>
</div>